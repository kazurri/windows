[user]
	name = kazurri
	email = kazurri@gmail.com

[color]
    ui = auto

[ghq]
	root = <ghq rootpath>

[credential "helperselector"]
	selected = wincred

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    keep-plus-minus-markers = true
    navigate = true
    side-by-side = true
    syntax-theme = OneHalfDark

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default

